{
  "name": "ResourceSet",
  "version": "0.0.1",
  "description": "ResourceSet",
  "homepage": "https://github.com/Testdozer/ResourceSet",
  "contributors": [
    {
      "name": "Radik Khisamov",
      "email": "radik_h@yahoo.com"
    },
    {
      "name": "Dmitry V. Abuzyarov",
      "email": "dvabuzyarov@gmail.com"
    }
  ],
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "i18n",
    "resource"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "main": "out/main.js",
  "private": true,
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -o",
    "electron-builders:build": "tsc -p ./projects/electron-builders/tsconfig.lib.json",
    "electron:tsc": "ng run main-process:build",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "utest": "tsc -p src/tsconfig.spec.json && cd out-spec && node unit-tests.index.js"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.0",
    "@angular/animations": "7.1.1",
    "@angular/cdk": "7.1.0",
    "@angular/cli": "7.1.0",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/compiler-cli": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/core-builds": "git://github.com/Testdozer/core-builds.git#6.0.x",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/language-service": "7.1.1",
    "@angular/material": "7.1.0",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "@ngrx/effects": "6.1.2",
    "@ngrx/entity": "6.1.2",
    "@ngrx/router-store": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@ngrx/store-devtools": "6.1.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.7.1",
    "chai": "^4.1.2",
    "codelyzer": "4.4.4",
    "core-js": "2.5.0",
    "electron": "2.0.7",
    "electron-builder": "20.28.1",
    "electron-reload": "1.2.5",
    "fast-deep-equal": "^2.0.1",
    "injection-js": "^2.2.1",
    "jasmine": "3.2.0",
    "jasmine-core": "3.2.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "4.2.1",
    "mock-require": "3.0.2",
    "moq.ts": "2.6.1",
    "ngrx-store-freeze": "0.2.4",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "proxyquire": "^2.0.1",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "rxjs-marbles": "4.3.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "uuid": "^3.3.2",
    "wait-on": "2.1.0",
    "webpack-node-externals": "^1.7.2",
    "zone.js": "0.8.26"
  }
}
